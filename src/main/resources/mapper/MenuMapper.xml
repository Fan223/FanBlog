<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="fan.fanblog.menu.dao.MenuDAO" >

    <update id="updateParentId">
        UPDATE
            menu
        SET
            parent_id = #{parentId}
        WHERE
            menu_id = #{menuId}
    </update>
</mapper>
